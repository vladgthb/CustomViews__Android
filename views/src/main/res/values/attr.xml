<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EditTextWithIcon">
        <attr name="image" format="integer" />
        <attr name="hint" format="string" />
        <attr name="textType" format="enum">
            <enum name="text" value="0" />
            <enum name="textPassword" value="1" />
            <enum name="textEmailAddress" value="2" />
            <enum name="textNumber" value="3" />
        </attr>
        <attr name="viewBackgroundColor" format="integer" />
        <attr name="iconPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

</resources>